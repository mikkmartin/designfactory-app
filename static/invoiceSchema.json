{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Invoice",
  "required": [
    "from",
    "fromDescription",
    "to",
    "description",
    "items",
    "myCompanyRegistrationNr",
    "ibanLabel",
    "ibanNr"
  ],
  "properties": {
    "fileName": {
      "$id": "#/properties/fileName",
      "type": "string",
      "default": "Arve_2019052201.pdf"
    },
    "from": {
      "$id": "#/properties/from",
      "type": "string",
      "default": "Ettevõte OÜ"
    },
    "invoiceNr": {
      "$id": "#/properties/invoiceNr",
      "type": "integer",
      "default": 2019052201
    },
    "date": {
      "$id": "#/properties/date",
      "type": "string",
      "default": "22-05-2019"
    },
    "dueDate": {
      "$id": "#/properties/dueDate",
      "type": "string",
      "default": "22-06-2019"
    },
    "fromDescription": {
      "$id": "#/properties/fromDescription",
      "type": "string",
      "default": "Kõvad veebivennad"
    },
    "to": {
      "$id": "#/properties/to",
      "type": "string",
      "default": "Klient OÜ"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "default": "Üks kõva veebipood"
    },
    "tax": {
      "$id": "#/properties/tax",
      "type": "number",
      "default": 0
    },
    "items": {
      "$id": "#/properties/items",
      "type": "array",
      "default": [
        [
          {
            "title": "Disain",
            "description": "Et asi oleks nice",
            "price": 4500,
            "quantity": 1
          },
          {
            "title": "Arendus",
            "description": "Et asi töötaks",
            "price": 4500,
            "quantity": 1
          }
        ]
      ],
      "additionalItems": true,
      "items": {
        "$id": "#/properties/items/items",
        "anyOf": [
          {
            "$id": "#/properties/items/items/anyOf/0",
            "type": "object",
            "default": {
              "title": "Disain",
              "description": "Et asi oleks nice",
              "price": 4500,
              "quantity": 1
            },
            "required": ["title", "description", "price"],
            "properties": {
              "title": {
                "$id": "#/properties/items/items/anyOf/0/properties/title",
                "type": "string",
                "title": "Name",
                "description": "Name of the product/service.",
                "default": "Disain"
              },
              "description": {
                "$id": "#/properties/items/items/anyOf/0/properties/description",
                "type": "string",
                "default": "Et asi oleks nice"
              },
              "price": {
                "$id": "#/properties/items/items/anyOf/0/properties/price",
                "type": "integer",
                "default": 4500
              },
              "quantity": {
                "$id": "#/properties/items/items/anyOf/0/properties/quantity",
                "type": "integer",
                "default": 1
              }
            },
            "additionalProperties": true
          }
        ]
      }
    },
    "paidInCash": {
      "$id": "#/properties/paidInCash",
      "type": "boolean",
      "default": false
    },
    "myCompanyRegistrationNr": {
      "$id": "#/properties/myCompanyRegistrationNr",
      "type": "number",
      "default": 12702285
    },
    "ibanLabel": {
      "$id": "#/properties/ibanLabel",
      "type": "string",
      "default": "LHV"
    },
    "ibanNr": {
      "$id": "#/properties/ibanNr",
      "type": "string",
      "default": "EE907700771001360230"
    },
    "website": {
      "$id": "#/properties/website",
      "type": "string",
      "examples": ["mikkmartin.co"],
      "default": ""
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "examples": ["mikk.martin@gmail.com"],
      "default": ""
    },
    "phoneNr": {
      "$id": "#/properties/phoneNr",
      "type": "string",
      "examples": ["+372 555 55 555"],
      "default": ""
    },
    "template": {
      "$id": "#/properties/template",
      "type": "string",
      "default": "qQJ7d5IKYTCVpaAMNptPH4"
    },
    "fonts": {
      "default": [],
      "description": "An array of custom fonts to load.",
      "examples": [
        [
          {
            "family": "Wigrum",
            "fonts": [
              {
                "src": "https://yourwebsite.com/fonts/Wigrum-Regular.ttf",
                "fontWeight": "Regular"
              }
            ]
          }
        ]
      ],
      "title": "Custom fonts",
      "additionalItems": true,
      "items": {
        "$id": "#/properties/fonts/items",
        "anyOf": [
          {
            "examples": [
              {
                "family": "Wigrum",
                "fonts": [
                  {
                    "src": "/fonts/Wigrum-Light.ttf",
                    "fontWeight": "light"
                  }
                ]
              }
            ],
            "required": ["family", "fonts"],
            "title": "Font object",
            "properties": {
              "family": {
                "examples": ["Wigrum"],
                "title": "Font family name"
              },
              "fonts": {
                "examples": [
                  [
                    {
                      "src": "/fonts/Wigrum-Light.ttf",
                      "fontWeight": "light"
                    }
                  ]
                ],
                "additionalItems": true,
                "items": {
                  "$id": "#/properties/fonts/items/anyOf/0/properties/fonts/items",
                  "anyOf": [
                    {
                      "examples": [
                        {
                          "src": "/fonts/Wigrum-Light.ttf",
                          "fontWeight": "light"
                        }
                      ],
                      "required": ["src", "fontWeight"],
                      "title": "Font weight object",
                      "properties": {
                        "src": {
                          "examples": ["/fonts/Wigrum-Light.ttf"],
                          "title": "Url"
                        },
                        "fontWeight": {
                          "default": "",
                          "examples": ["Regular"],
                          "title": "Font weight"
                        }
                      },
                      "additionalProperties": false
                    }
                  ]
                }
              }
            },
            "additionalProperties": true
          }
        ]
      }
    }
  },
  "additionalProperties": false
}
